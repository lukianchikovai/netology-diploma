server.port: 5601
server.host: "0.0.0.0"
elasticsearch.hosts: ["http://{{ elasticsearch_host }}:9200"]
kibana.index: ".kibana"
logging.dest: /var/log/kibana/kibana.log

# Security settings
elasticsearch.username: "{{ kibana_username }}"
elasticsearch.password: "{{ kibana_password }}"
xpack.security.enabled: true
